<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="85.0" prefWidth="600.0" spacing="40.0" style="-fx-background-color: #383838;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.spotify.PlayBarController">
   <children>
      <ImageView fx:id="musicCoverImage" fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="playerBarName" text="music name" textFill="WHITE">
         <padding>
            <Insets top="15.0" />
         </padding>
      </Label>
      <Button fx:id="previousButt" mnemonicParsing="false" onMouseClicked="#previousButtC" prefHeight="54.0" prefWidth="65.0" style="-fx-background-color: #383838;" text="⏪" textFill="WHITE">
         <font>
            <Font size="31.0" />
         </font>
      </Button>
      <Button fx:id="playButt" mnemonicParsing="false" onMouseClicked="#playButtC" style="-fx-background-color: #383838;" text="▶" textFill="WHITE">
         <font>
            <Font size="31.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
      <Button fx:id="nextButt" mnemonicParsing="false" onMouseClicked="#nextButt" style="-fx-background-color: #383838;" text="⏩" textFill="WHITE">
         <font>
            <Font size="31.0" />
         </font>
      </Button>
      <Label text="Label" />
   </children>
</HBox>
